/* pages/goods/index.wxss */
.goodsimgs{
  width: 100%;
  height: 750rpx;
}
.goodsimgs image{
  width: 100%;
  height: 100%;
}
.goods_name{
  height: 90rpx;
  background-color: lightgrey;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
}
.goods_name .left{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  font-size: 30rpx;
  height: 40rpx;
  width: calc(100% - 130rpx);
}
.goods_name .right{
  color: white;
  font-size: 30rpx;
  font-weight: bold;
  height: 90rpx;
  line-height: 90rpx;
  width: 130rpx;
  padding-left: 20rpx;
  border-top-left-radius: 50rpx;
  border-bottom-left-radius: 50rpx;
  background-image: linear-gradient(to right, #f3d10e 0%, #f48f18 100%);
}
.goods-info{
  background: white;
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.goods-info .desc{
  font-size: 26rpx;
  color: gray;
  padding-top: 10rpx;
}
.goods-info .price{
  margin-top: 10rpx;
  display: flex;
  align-items: center;
  justify-content: start;
  margin-bottom: 40rpx;
}
.goods-info  .retail-price{
  color: red;
  font-size: 32rpx;
}
.goods-info  .counterPrice{
  text-decoration: line-through;
  padding-left: 20rpx;
  color: gray;
 
}
.goods-info .brand text{
  color: #f48f18;
  border-radius: 10rpx;
  font-size: 26rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  border: #f48f18 solid 1rpx;
}
.goods-info .line{
  margin-top: 40rpx;
  height: 1rpx;
  background: #f48f18;
}
.section-nav{
  height: 100rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  background: white;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-weight: bold;
}
.section-nav image{
  width: 40rpx;
  height: 40rpx;
}
.goods-attr{
  margin-top: 20rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: 20rpx;
  background: white;
}
.goods-attr .title{
  height: 80rpx;
  background: white;
  font-size: 26rpx;
  font-weight: bold;
  line-height: 80rpx;
}
.goods-attr .item{
  padding: 15rpx;
  background: lightgray;
  border-bottom: white solid 3rpx;
  display: flex;
  justify-content: start;
}
.goods-attr .item .right{
  padding-left: 40rpx;
  font-size: 26rpx;
  white-space: nowrap;  
text-overflow:ellipsis;
overflow:hidden;
height: 40rpx;
  width: calc(100% - 140rpx);
}
.goods-attr .item .left{
  font-size: 26rpx;
  line-height: 50rpx;
  font-weight: bold;
  width: 110rpx;
  max-lines: 1;
  height: 40rpx;
  white-space: nowrap;  
text-overflow:clip;
overflow:hidden;
}
.detail .item{
  width: 100%;
  height: 750rpx;
  background: white;
}
.detail .item image{
  width: 100%;
  height: 100%;
}
.common-problem{
  background: white;
  padding: 20rpx;
}
.header{
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.left{
  width: 150rpx;
  height: 2rpx;
  background: lightgrey;
}
.title{
  font-size: 28rpx;
  width: 180rpx;
  text-align: center;
}
.right{
  width: 150rpx;
  height: 2rpx;
  background: lightgrey;
}
.common-problem .item{
  padding-bottom: 25rpx;
}
.common-problem .item .question-box{
  display: flex;
  align-items: center;
  justify-content: start;
  margin-bottom: 20rpx;
}
.common-problem .item .spot{
  float: left;
  height: 8rpx;
  width: 8rpx;
  background: #b4282d;
  border-radius: 50%;
}
.common-problem .item .question{
  padding-left: 10rpx;
  font-size: 26rpx;
}
.common-problem .item .answer{
  padding-left: 18rpx;
  font-size: 24rpx;
  color: gray;
}
.related-goods .header{
  background: white;
  
}
.related-goods{
  overflow: hidden;
  background: white;
  border-bottom: lightgray solid 50rpx;
}
.related-goods .content{
  margin-top: 20rpx;
}
.related-goods .item{
  width: calc(50% - 5rpx);
  float: left;
  border-top: lightgray solid 5rpx;
  border-right: lightgray solid 5rpx;
  text-align: center;
  overflow: hidden;
  /* border-right: lightgray solid 6rpx;
  border-bottom: lightgray solid 6rpx; */
}

.related-goods .item-b{
  border-right: none;
  /* border-left: lightgray solid 6rpx;
  border-bottom: lightgray solid 6rpx; */
}
.related-goods .item .name{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  width: 302rpx;
  font-size: 26rpx;
}
.related-goods .item .img{
  padding-top: 10rpx;
  width: 302rpx;
  height: 302rpx;
}
.related-goods .item .price{
  font-size: 26rpx;
  color: #b4282d;
  padding-bottom: 10rpx;
}
.contact {
  height: 80rpx;
  width: 80rpx;
  border-radius: 100%;
  position: fixed;
  bottom: 106rpx;
  right: 10rpx;
  font-size: 20rpx;
  background: url("data:image/png;base64,") no-repeat center 11rpx;
  background-size: 55rpx auto;
}
.bottom-btn{
  position: fixed;
  height: 100rpx;
  width: 100%;
  bottom: 0;
  left: 0;
  display: flex;
  align-content: center;
  justify-content: start;
  background: white;
  z-index: 1;
}
.bottom-btn .collect{
  flex: 2;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  border-right: lightgray solid 1rpx;
  border-top: lightgray solid 1rpx;
}
.bottom-btn .collect .icon{
width: 50rpx;
height: 50rpx;
}
.bottom-btn .cart{
  width: 120rpx;
  flex: 2.5;
  display: flex;
  align-items: center;
  justify-content: center;
  border-top: lightgray solid 1rpx;
}
.bottom-btn .cart image{
width: 50rpx;
height: 50rpx;
}
.bottom-btn .r{
flex: 4;
display: flex;
padding-top: 20rpx;
  justify-content: center;
background: lightsalmon;
color: white;
}
.bottom-btn .c{
  flex: 4;
  display: flex;
  padding-top: 20rpx;
  justify-content: center;
  background: red;
  color: white;
}
.share-pop-box{
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  position: fixed;
bottom: 0;
}
.share-pop-box .share-pop{
  position: fixed;
  bottom: 100rpx;
width: 100%;
height: 400rpx;
background: white;
}
.share-pop-box .close .icon{
  width: 50rpx;
  height: 50rpx;
}
.share-pop-box .close{
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}
.share-pop-box .share-info{
  display: flex;
  margin-top: 110rpx;
  align-items: center;
  justify-content: space-between;
}
.share-pop-box .shareBtn{
  background: white;
}
.shareBtn::after {
  border: none;
  border-radius: 0%;
}
.share-pop-box .shareBtn .text{
  font-size: 26rpx;
  margin-top: -20rpx;
}
.share-pop-box .shareBtn .image{
  margin-top: 10rpx;
  width: 100rpx;
  height: 100rpx;
}
.share-pop-box .save {
  background: white;
}
.share-pop-box .save .image{
  margin-top: 10rpx;
  width: 100rpx;
  height: 100rpx;
}
.share-pop-box .save .text{
  font-size: 26rpx;
  margin-top: -30rpx;
}
.save::after {
  border: none;
  border-radius: 0%;
}

.attr-pop-box{
  background: rgba(0, 0, 0, 0.5);
  width: 100%;
  height: 100%;
  position: fixed;
  bottom: 0;
}
.attr-pop{
  height: 80%;
  width: calc(100% - 60rpx);
  background: white;
  position: fixed;
  bottom: 100rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  border-top-left-radius: 15rpx;
  border-top-right-radius: 15rpx;
  overflow: auto;
}
.attr-pop-box .close{
  float: right;
  right: 30rpx;
  margin-top: 30rpx;
}
.attr-pop-box .close .icon{
  width: 50rpx;
  height: 50rpx;
}
.img-info{
padding-top: 30rpx;
display: flex;
align-items: center;
justify-content: start;
}
.img-info .img{
  height: 200rpx;
  width: 200rpx;
  margin-right: 30rpx;
}
.spec-con{
  margin-top: 40rpx;
}
.spec-con .name{
  font-size: 29rpx;
  margin-bottom: 25rpx;
}
.spec-con .values{
  margin-bottom: 30rpx;
}
.spec-con .value{
  font-size: 26rpx;
  padding: 15rpx;
  color: #999;
  display: inline-block;
  text-align: center;
  margin-right: 25rpx;
  margin-bottom: 15rpx;
  border-radius: 6rpx;
  border: 1rpx solid #999;
}
.spec-con .value.disable {
  border: 1px solid #ccc;
  color: #ccc;
}

.spec-con .value.selected {
  border: 1px solid #f0070e;
  color: #fff;
  background: #f0070e;
}
.sel-num{
  width: 300rpx;
  display: flex;
  align-items: center;
  justify-content: start;
  margin-bottom: 30rpx;
}
.sel-num .cut{
  flex: 2;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-top: 1rpx solid #999;
  border-left: 1rpx solid #999;
  border-bottom: 1rpx solid #999;
}
.sel-num .number{
  flex: 3;
  height: 60rpx;
  border: 1rpx solid #999;
  text-align: center;
}
.sel-num .add{
  flex: 2;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-top: 1rpx solid #999;
  border-right: 1rpx solid #999;
  border-bottom: 1rpx solid #999;
}
